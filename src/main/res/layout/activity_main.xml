<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tlo"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="206dp"
        android:layout_height="190dp"
        android:layout_marginStart="67dp"
        android:layout_marginEnd="67dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/zd1" />

    <ImageButton
        android:id="@+id/iB1"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/sud1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/iB2"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/memory"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/memory"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/iB3"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/mat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iB1"
        app:srcCompat="@drawable/mat"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>